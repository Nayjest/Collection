{
    "name": "nayjest/collection",
    "description": "Collections for PHP",
    "keywords": [
        "collection",
        "collections"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vitalii [Nayjest] Stepanenko",
            "email": "mail@vitaliy.in",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "nayjest/manipulator": "^2||^3",
        "evenement/evenement": "2.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4||^5",
        "danielstjules/stringy": "^1.9||^2"
    },
    "suggest": {
        "danielstjules/stringy": "Required to filter ObjectCollection by properties with getters"
    },
    "autoload": {
        "psr-4": {
            "Nayjest\\Collection\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nayjest\\Collection\\Test\\": "tests"
        }
    },
    "support": {
        "email": "mail@vitaliy.in"
    },
    "scripts": {
        "test": "php vendor/phpunit/phpunit/phpunit"
    },
    "minimum-stability": "dev"
}
